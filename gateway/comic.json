{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "timeout": "10s",
  "cache_ttl": "300ms",
  "output_encoding": "json",
  "name": "projectf",
  "port": 8080,
  "endpoints": [
    {
      "endpoint": "/comic",
      "method": "GET",
      "timeout": "1000s",
      "backend": [
        {
          "host": [
            "http://localhost:8080"
          ],
          "url_pattern": "/b",
          "allow": [
            "datatype",
            "rawdata"
          ],
          "extra_config": {
            "validation/cel": [
              {
                "check_expr": "resp_data.datatype == 'Character'"
              }
            ]
          }
        }
      ]
    },
    {
      "endpoint": "/b",
      "timeout": "200ms",
      "method": "GET",
      "backend": [
        {
          "host": [
            "amqp://guest:guest@localhost:5672"
          ],
          "disable_host_sanitize": true,
          "extra_config": {
            "backend/amqp/consumer": {
              "name": "queue-1",
              "exchange": "producer-q-exchange",
              "durable": true,
              "delete": false,
              "no_wait": true,
              "no_local": false,
              "routing_key": [
                "1"
              ],
              "prefetch_count": 10,
              "auto_ack": false,
              "backoff_strategy": "exponential-jitter"
            }
          }
        }
      ]
    }
  ]
}